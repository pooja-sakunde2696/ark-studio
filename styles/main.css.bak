.main{
    background-color: white;
    font-family: Arial, Helvetica, sans-serif;
}
.transparent_nav{
  
 
  opacity: 0.7;
  background-color:rgba(255,255,255,0.78);

}
.nav_bar{
    display: inline;
    float: right;
	 
}
#nav_list{
    text-align: center;
}
.nav_bar li{
    list-style-type: none;
    display: inline;
    text-decoration: none;
    padding:10px;

}
.nav_bar li a{
    text-decoration: none;
    color: black;
}
.top-left {
    position: absolute;
    top: 8px;
    left: 16px;
  }
  .top-right {
    position: absolute;
    top: 8px;
    right: 16px;
  }
  .cta{
      background: white;
  }
  .cta #cta_text{
      font-size: 16px;
      position: absolute;
    bottom: 8px;
    left: 16px;
  }
  .cta a{
    padding:175px;
    text-align: center; 
    
  }
  .cta_link{
      display: block;
      text-decoration: none;
      color:white;
      background-color: blue;
      list-style-type: none;
      

  }
  .button{
    background-color: #008CBA;
    border: none;
    text-decoration: none;
    display: inline-block;
    text-align: center;
    padding:5px;
    margin: 5px;
  }
  .bottom-left {
    position: absolute;
    bottom: 8px;
    left: 16px;
    opacity: 0.7;
  }
  .bottom-right {
    position: absolute;
    bottom: 32px;
    right: 16px;
    opacity: 0.7;
  }
