 #flex{
width:1200px;
height:500px;
display: flex;

 }
 
#flex div{
  flex: 1;
}
.display{
  height:500px;
  width: 350px;
}
.servicework{
  height: 500px;
  display: flex;
   
}
.servicework img{
  flex: 1;
}
.testimonals wrapper{
  text-align: center;
  height: 500px;
  width: 800px;
  box-sizing: border-box;
}
.testimonals wrapper h1{
  padding: 50px;
  text-align: center;
}
.testimonals wrapper ul{
  list-style-type: none;
  display: inline;
  text-decoration: none;
  overflow: hidden;
}
.testimonals wrapper ul li{
  padding:20px;
  display: inline;
  list-style-type:none;
  text-decoration: none;
  text-align: center;
  align-content: center;
  flex: 1;
}
.testimonals wrapper img{
  list-style-type: none;
  border-radius: 50%;
}
.logowrapper{
  padding: 0px;
  margin: 0px;
  height: 70px;
  width: auto;
  background-color: rgb(190,190,190);
}
.logowrapper ul{
  padding: 0;
  list-style-type: none;
  text-decoration: none;
  float: right;
  
}
.logowrapper ul li{
  padding-left:80px;
}
